# API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# Environment
NODE_ENV=development

# Application
APP_NAME=Trade Manager Pro
APP_VERSION=1.0.0

# Trading Settings
DEFAULT_TRADE_AMOUNT=10
MAX_DAILY_LOSS=100
MAX_DAILY_PROFIT=500
MAX_CONSECUTIVE_LOSSES=3

# Analysis Settings
MIN_CONFIDENCE_THRESHOLD=70
ANALYSIS_INTERVAL=60000 # 1 minute in milliseconds

# Logging
LOG_LEVEL=info
ENABLE_DEBUG_LOGS=true 